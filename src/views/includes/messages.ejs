<% if (errors.length > 0) { %>

  <div class="alert alert-danger">
    <br>
    <% errors.forEach(erro => { %>
    <%= erro %><br>
    <br>
    <% }); %>
  </div>

<% } %>
  
<% if (success.length > 0) { %>
  <div class="alert alert-success">
    <% success.forEach(success => { %>
    <%= success %><br>
    <% }); %>
  </div>
<% } %>